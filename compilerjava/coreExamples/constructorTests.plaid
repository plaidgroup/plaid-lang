package coreExamples.constructorTests;

state Test {
	var foo;
	
	method baz() {
		this <- Test with { var qux = fn () => { java.lang.System.out.println(this.foo); }; var foo = "bar"; };
	}
	
	method print() {
		java.lang.System.out.println(this.foo);
	}
}

method main() {
	val t = new Test with {
		var foo = "foo";
	};
	
	t.baz();
	t.qux();
	t.print();
}