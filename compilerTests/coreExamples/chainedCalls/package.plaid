package coreExamples.chainedCalls;

state S {
	var t = new T;
	
	method foo() {
		new T;
	}
}

state T {
	method bar() {
		23;
	}
}

method main() {
	var line = java.io.BufferedReader.new(java.io.FileReader.new("test.txt")).readLine();

	java.lang.System.out.println("This should say \"plaid\": " + line);
	java.lang.System.out.println("This should be 23: " + (new S).foo().bar());
	
}