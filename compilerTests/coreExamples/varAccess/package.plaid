package coreExamples.varAccess;

method foo(x, y) {
	x + y;
}

method bar() {
	var x = 10;
	val f = { x + 1; };
}

method qux() {
	var x = 0;
	
	val f = {
		var x = 10;
		x;
	};
	
	f();
}

method main() {
	var x = 0;
	x = 5;

	var y = foo(x, 200);
	printLine("This should be 205: " + y);
		
	val f = { x + 1; };
	
	val g = fn (a) => { x + a; };
}